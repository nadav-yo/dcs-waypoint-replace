<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<StackPane xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1"
           fx:controller="org.faulty.wpreplace.ui.LoadMissionController"
           stylesheets="@/org/faulty/wpreplace/ui/style.css">
    <ImageView>
        <Image url="/org/faulty/wpreplace/ui/icons/logo.jpg" requestedHeight="300" preserveRatio="true"/>
    </ImageView>

    <HBox alignment="BOTTOM_CENTER" spacing="120">
        <Button text="Select Mission" onAction="#loadFileAndContinue" styleClass="button-styled"/>
    </HBox>
</StackPane>
